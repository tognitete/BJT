{
    "manifest_version": 2,
    "name": "définir le nom ",
    "short_name": "plugin à définir",
    "description": "définir la decription",
    "version": "1.0",
    "minimum_chrome_version": "38",
    "content_scripts": [{
      "matches": [ "https://*/*", "http://*/*" ],
        "js": ["inject.js"],
        "run_at": "document_end"
    }],

    "permissions": [
	
        "notifications",
        "http://*/*",
        "https://*/*",
	      "contextMenus",
        "tabs"
    ],

    "icons": {
        "16": "assets/icone.png",
        "48": "assets/icone.png"
    },

    "browser_action": {
        "default_icon": "assets/icone.png",
        "default_title": "extension audio"
		
    },
    "background": { "scripts": ["background.js"] }
	
}
